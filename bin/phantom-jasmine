#!/usr/bin/env node

var exec = require('child_process').exec;
var path = require('path');
var lib = path.join(__dirname, './../lib');

var cmds = [
  'phantomjs',
  path.join(lib, 'run_jasmine_test.coffee'),
  process.argv[2]
];

exec(cmds.join(' '), function(err, stdout, stderr) {
  console.log(stdout);
});
